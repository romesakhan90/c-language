#include<stdio.h>
#include<conio.h>
int main ()

{
	int percentage;
	printf ("enter your percentage");
	scanf("%d/n,percentage");
	if (percentage>80)
	{ printf("you got A+");
	}
	else if (percentage>=70||percentage<=79)
	{ printf("you got A");
	}
	
	else if (percentage>=60||percentage<=69)
	{ printf("you got B");
	}
	
	else if (percentage>=60||percentage<=59)
	{ printf("you got c");
	}
	else 
	{ printf ("wrong answer");
	}
	return 0;
}
